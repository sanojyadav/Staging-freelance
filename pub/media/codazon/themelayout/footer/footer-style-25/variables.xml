                        &lt;/div&gt;&#xD;
                    &lt;/div&gt;&#xD;
                    &lt;div class="col-sm-12"&gt;&#xD;
                        &lt;div class="footer-payment"&gt;&lt;img class="img-responsive" src="{{media url="wysiwyg/codazon/footer-payment.png"}}" alt="Accept Payment"&gt;&lt;/div&gt;&#xD;
                    &lt;/div&gt;&#xD;
                &lt;/div&gt;&#xD;
            &lt;/div&gt;&#xD;
        &lt;/div&gt;&#xD;
