  <content>&lt;div class="footer-style-25 container-fluid"&gt;&#xD;
    &lt;div class="main-footer"&gt;&#xD;
        &lt;div class="service-banner-style06"&gt;&#xD;
            &lt;div class="container"&gt;&#xD;
                &lt;div class="row"&gt;&#xD;
                    &lt;div class="item col-sm-6 col-xs-12"&gt;&#xD;
                        &lt;p class="item-icon"&gt;&lt;span class="fa-lock fa"&gt;&amp;nbsp;&lt;/span&gt;&lt;/p&gt;&#xD;
                        &lt;div class="service-desc"&gt;&#xD;
                            &lt;p class="service-subtitle"&gt;100% Secured&lt;/p&gt;&#xD;
                            &lt;p&gt;Payment&lt;/p&gt;&#xD;
                        &lt;/div&gt;&#xD;
                    &lt;/div&gt;&#xD;
                    &lt;div class="item col-sm-6 col-xs-12"&gt;&#xD;
                        &lt;p class="item-icon"&gt;&lt;span class="fa-phone fa"&gt;&amp;nbsp;&lt;/span&gt;&lt;/p&gt;&#xD;
                        &lt;div class="service-desc"&gt;&#xD;
                            &lt;p class="service-subtitle"&gt;Customer Service&lt;/p&gt;&#xD;
                            &lt;p&gt;Mail &amp;amp; Telephone&lt;/p&gt;&#xD;
                        &lt;/div&gt;&#xD;
                    &lt;/div&gt;&#xD;
                    &lt;div class="item col-sm-6 col-xs-12"&gt;&#xD;
                        &lt;p class="item-icon"&gt;&lt;span class="fa-exchange fa"&gt;&amp;nbsp;&lt;/span&gt;&lt;/p&gt;&#xD;
                        &lt;div class="service-desc"&gt;&#xD;
                            &lt;p class="service-subtitle"&gt;14 Days to Change&lt;/p&gt;&#xD;
                            &lt;p&gt;Your Mind&lt;/p&gt;&#xD;
                        &lt;/div&gt;&#xD;
                    &lt;/div&gt;&#xD;
                    &lt;div class="item col-sm-6 col-xs-12"&gt;&#xD;
                        &lt;p class="item-icon"&gt;&lt;span class="fa-thumbs-o-up fa"&gt;&amp;nbsp;&lt;/span&gt;&lt;/p&gt;&#xD;
                        &lt;div class="service-desc"&gt;&#xD;
                            &lt;p class="service-subtitle"&gt;Highest Quality&lt;/p&gt;&#xD;
                            &lt;p&gt;Guarantee&lt;/p&gt;&#xD;
                        &lt;/div&gt;&#xD;
                    &lt;/div&gt;&#xD;
                &lt;/div&gt;&#xD;
            &lt;/div&gt;&#xD;
        &lt;/div&gt;&#xD;
