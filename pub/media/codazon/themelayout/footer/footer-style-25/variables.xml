                                    &lt;li&gt;&lt;a href="{{store url='search/term/popular'}}"&gt;Search Terms&lt;/a&gt;&lt;/li&gt;&#xD;
                                    &lt;li&gt;&lt;a href="{{store url='catalogsearch/advanced'}}"&gt;Advanced Search&lt;/a&gt;&lt;/li&gt;&#xD;
                                    &lt;li&gt;&lt;a href="{{store url='sales/guest/form'}}"&gt;Orders and Returns&lt;/a&gt;&lt;/li&gt;&#xD;
                                    &lt;li&gt;&lt;a href="{{store url='blog'}}"&gt;Lastest New&lt;/a&gt;&lt;/li&gt;&#xD;
                                    &lt;li&gt;&lt;a href="{{store url='contact'}}"&gt;Out Sitemap&lt;/a&gt;&lt;/li&gt;&#xD;
                                    &lt;li class="last"&gt;&lt;a href="{{store url='contact'}}"&gt;Contact Us&lt;/a&gt;&lt;/li&gt;&#xD;
                                &lt;/ul&gt;&#xD;
                            &lt;/div&gt;&#xD;
                        &lt;/div&gt;&#xD;
                        &lt;div class="col-sm-6"&gt;&#xD;
                            &lt;div class="footer-box"&gt;&#xD;
                                &lt;p class="h5" data-cdz-toggle="#footer-content-2"&gt;Customer Suport&lt;/p&gt;&#xD;
                                &lt;ul id="footer-content-2" class="footer-link showhide hover-link01"&gt;&#xD;
                                    &lt;li&gt;&lt;a href="#"&gt;Help &amp;amp; FAQs&lt;/a&gt;&lt;/li&gt;&#xD;
                                    &lt;li&gt;&lt;a href="{{store url='contact'}}"&gt;Contact Us&lt;/a&gt;&lt;/li&gt;&#xD;
                                    &lt;li&gt;&lt;a href="#"&gt;Instagram profile&lt;/a&gt;&lt;/li&gt;&#xD;
                                    &lt;li&gt;&lt;a href="#"&gt;Returns&lt;/a&gt;&lt;/li&gt;&#xD;
                                    &lt;li&gt;&lt;a href="#"&gt;Privacy Policy&lt;/a&gt;&lt;/li&gt;&#xD;
                                    &lt;li&gt;&lt;a href="#"&gt;Terms &amp;amp; Conditions&lt;/a&gt;&lt;/li&gt;&#xD;
                                    &lt;li&gt;&lt;a href="#"&gt;Order History&lt;/a&gt;&lt;/li&gt;&#xD;
                                    &lt;li class="last"&gt;&lt;a href="#"&gt;Sample Links&lt;/a&gt;&lt;/li&gt;&#xD;
                                &lt;/ul&gt;&#xD;
                            &lt;/div&gt;&#xD;
                        &lt;/div&gt;&#xD;
                        &lt;div class="col-sm-6"&gt;&#xD;
                            &lt;div class="footer-box bg-box"&gt;&#xD;
                                &lt;div class="footer-app"&gt;&#xD;
                                    &lt;p class="app-title h5"&gt;Download App&lt;/p&gt;&#xD;
                                    &lt;div class="footer-app-store"&gt;&lt;a href="#"&gt;&lt;img src="{{media url="wysiwyg/codazon/footer-img-02.jpg"}}" alt="Codazon_alt"&gt; &lt;/a&gt; &lt;a href="#"&gt;&lt;img src="{{media url="wysiwyg/codazon/footer-img-01.jpg"}}" alt="Codazon_alt"&gt; &lt;/a&gt;&lt;/div&gt;&#xD;
                                &lt;/div&gt;&#xD;
                                &lt;div class="newsletter"&gt;&#xD;
                                    &lt;p class="newsletter-title h5"&gt;Subscribe Us&lt;/p&gt;&#xD;
                                    &lt;div&gt;&#xD;
                                        &lt;div class="newsletter-content"&gt;{{block class="Magento\Newsletter\Block\Subscribe" name="cdz.newsletter" template="Magento_Newsletter::subscribe.phtml"}}&lt;/div&gt;&#xD;
                                    &lt;/div&gt;&#xD;
                                &lt;/div&gt;&#xD;
                            &lt;/div&gt;&#xD;
                        &lt;/div&gt;&#xD;
                    &lt;/div&gt;&#xD;
                    &lt;div class=" middle-bottom nav-footer-content text-center"&gt;&#xD;
                        &lt;ul class="items nav-links hover-link01"&gt;&#xD;
                            &lt;li class="nav-link"&gt;&lt;a href="#"&gt;Search&lt;/a&gt; &lt;a href="#"&gt;Blog&lt;/a&gt; &lt;a href="#"&gt;Products&lt;/a&gt; &lt;a href="#"&gt;Faq&lt;/a&gt; &lt;a href="#"&gt;Lookbook&lt;/a&gt; &lt;a href="#"&gt;Portfolio&lt;/a&gt; &lt;a href="#"&gt;Categories&lt;/a&gt;&lt;/li&gt;&#xD;
                            &lt;li class="nav-link"&gt;&lt;a href="#"&gt;Contact&lt;/a&gt; &lt;a href="#"&gt;About Us&lt;/a&gt; &lt;a href="#"&gt;Wishlist&lt;/a&gt; &lt;a href="#"&gt;Login&lt;/a&gt; &lt;a href="#"&gt;Create Account&lt;/a&gt;&lt;/li&gt;&#xD;
                        &lt;/ul&gt;&#xD;
                    &lt;/div&gt;&#xD;
                &lt;/div&gt;&#xD;
            &lt;/div&gt;&#xD;
