<?xml version="1.0"?>
<config>
  <identifier>footer-style-31</identifier>
  <title>Footer Style 31</title>
  <variables>
    <footer_background>transparent</footer_background>
    <footer_background_file>/b/g/bg-footer_2.jpg</footer_background_file>
    <footer_background_image_option>center bottom no-repeat</footer_background_image_option>
    <footer_background_normal>#ffffff</footer_background_normal>
    <footer_border>#3a3a3a</footer_border>
    <footer_bottom_background>transparent</footer_bottom_background>
    <footer_font>14px</footer_font>
    <footer_hover_text>#c26431</footer_hover_text>
    <footer_text>#ffffff</footer_text>
    <footer_text_normal>#121212</footer_text_normal>
    <footer_title_font>18px</footer_title_font>
    <footer_title_text>#ffffff</footer_title_text>
    <footer_top_background>transparent</footer_top_background>
  </variables>
  <layout_xml>&lt;body&gt;&#xD;
    &lt;referenceBlock name="footer_links" remove="true" /&gt;&#xD;
    &lt;referenceBlock name="copyright" remove="true" /&gt;&#xD;
    &lt;referenceBlock name="report.bugs" remove="true" /&gt;&#xD;
    &lt;referenceBlock name="form.subscribe" remove="true" /&gt;&#xD;
    &lt;move element="theme-layout-footer" destination="footer-container" /&gt;&#xD;
&lt;/body&gt;</layout_xml>
  <custom_variables/>
  <custom_fields>
    <version>66011c9e44789</version>
    <auto_detect_files>
      <item>_pe-icon-7-stroke.less.css</item>
    </auto_detect_files>
    <custom_variables>@service-style06-background: #125430;&#xD;
@service-style06-icon-background: #fc0;&#xD;
@service-style06-icon-text: #fff;&#xD;
@service-style06-text: #fff;&#xD;
@service-style06-icon-hover-background: #fc0;&#xD;
@service-style06-icon-hover-text: #121212;</custom_variables>
    <custom_less_code></custom_less_code>
    <custom_css_code></custom_css_code>
    <flexible_less/>
  </custom_fields>
  <parent></parent>
  <content>&lt;div class="footer-style-31"&gt;&#xD;
&lt;div class="main-footer"&gt;&#xD;
&lt;div class="footer-content-middle text-center"&gt;&#xD;
&lt;div class="container"&gt;&#xD;
&lt;div class="footer content"&gt;&#xD;
&lt;div class="row middle-top"&gt;&#xD;
&lt;div class="col-sm-8"&gt;&#xD;
&lt;div class=" footer-box"&gt;&#xD;
&lt;p class="h5" data-cdz-toggle="#footer-content-1"&gt;Contact&lt;/p&gt;&#xD;
&lt;div class="footer-content-1"&gt;&#xD;
&lt;ul class="footer-link"&gt;&#xD;
&lt;li class="f-address"&gt;&lt;span class="pe-7s-map-marker"&gt;&amp;nbsp;&lt;/span&gt;Montergade 19 1140, Copenhagen, Denmark&lt;/li&gt;&#xD;
&lt;li class="f-phone"&gt;&lt;span class="pe-7s-call"&gt;&amp;nbsp;&lt;/span&gt;&lt;a href="tel:+1123 456 xxx"&gt;+123 456 xxx&lt;/a&gt;&lt;/li&gt;&#xD;
&lt;li class="f-email"&gt;&lt;span class="pe-7s-mail-open"&gt;&amp;nbsp;&lt;/span&gt;codazon@gmail.com&lt;/li&gt;&#xD;
&lt;li class="f-time"&gt;&lt;span class="pe-7s-timer"&gt;&amp;nbsp;&lt;/span&gt;Week 7 days from 7:00 to 20:00&lt;/li&gt;&#xD;
&lt;/ul&gt;&#xD;
&lt;/div&gt;&#xD;
&lt;/div&gt;&#xD;
&lt;/div&gt;&#xD;
&lt;div class="col-sm-8"&gt;&#xD;
&lt;div class="footer-box footer-box-line"&gt;&#xD;
&lt;div class="footer-logo"&gt;&lt;img src="{{media url="wysiwyg/codazon/footer_logo_white.png"}}" alt="Accept Payment"&gt;&lt;/div&gt;&#xD;
&lt;p&gt;Lorem Ipsum is simply dummy text of the printing and typesetting industry.&lt;/p&gt;&#xD;
&lt;/div&gt;&#xD;
&lt;/div&gt;&#xD;
&lt;div class="col-sm-8"&gt;&#xD;
&lt;div class="footer-box bg-box"&gt;&#xD;
&lt;div class="newsletter"&gt;&#xD;
&lt;p class="newsletter-title h5"&gt;Subscribe Us&lt;/p&gt;&#xD;
&lt;div&gt;&#xD;
&lt;div class="newsletter-content"&gt;{{block class="Magento\Newsletter\Block\Subscribe" name="cdz.newsletter" template="Magento_Newsletter::subscribe.phtml"}}&lt;/div&gt;&#xD;
&lt;/div&gt;&#xD;
&lt;/div&gt;&#xD;
&lt;div class="footer-app"&gt;&#xD;
&lt;p class="app-title h5"&gt;Download App&lt;/p&gt;&#xD;
&lt;div class="footer-app-store"&gt;&lt;a href="#"&gt;&lt;img src="{{media url="wysiwyg/codazon/footer-img-02.jpg"}}" alt="Codazon_alt"&gt; &lt;/a&gt; &lt;a href="#"&gt;&lt;img src="{{media url="wysiwyg/codazon/footer-img-01.jpg"}}" alt="Codazon_alt"&gt; &lt;/a&gt;&lt;/div&gt;&#xD;
&lt;/div&gt;&#xD;
&lt;/div&gt;&#xD;
&lt;/div&gt;&#xD;
&lt;/div&gt;&#xD;
&lt;div class=" middle-bottom nav-footer-content text-center"&gt;&#xD;
&lt;div class="footer-social"&gt;&#xD;
&lt;p class="social"&gt;&lt;a href="#"&gt;&lt;span class="fa fa-facebook"&gt;facebook&lt;/span&gt;&lt;/a&gt; &lt;a href="#"&gt;&lt;span class="fa fa-x-twitter"&gt;twitter&lt;/span&gt;&lt;/a&gt; &lt;a href="#"&gt;&lt;span class="fa fa-pinterest-p"&gt;pinterest&lt;/span&gt;&lt;/a&gt; &lt;a href="#"&gt;&lt;span class="fa fa-google-plus"&gt;google plus&lt;/span&gt;&lt;/a&gt; &lt;a href="#"&gt;&lt;span class="fa fa-rss"&gt;rss&lt;/span&gt;&lt;/a&gt;&lt;/p&gt;&#xD;
&lt;/div&gt;&#xD;
&lt;/div&gt;&#xD;
&lt;/div&gt;&#xD;
&lt;/div&gt;&#xD;
&lt;/div&gt;&#xD;
&lt;div class="footer-content-bottom"&gt;&#xD;
&lt;div class="container"&gt;&#xD;
&lt;div class="row"&gt;&#xD;
&lt;div class="col-sm-12"&gt;&#xD;
&lt;div class="footer-address"&gt;&#xD;
&lt;p&gt;&#xA9; 2021 Codazon Ltd. Trademarks and brands&lt;/p&gt;&#xD;
&lt;/div&gt;&#xD;
&lt;/div&gt;&#xD;
&lt;div class="col-sm-12"&gt;&#xD;
&lt;div class="footer-payment"&gt;&lt;img class="img-responsive" src="{{media url="wysiwyg/codazon/footer-payment.png"}}" alt="Accept Payment"&gt;&lt;/div&gt;&#xD;
&lt;/div&gt;&#xD;
&lt;/div&gt;&#xD;
&lt;/div&gt;&#xD;
&lt;/div&gt;&#xD;
&lt;/div&gt;&#xD;
&lt;/div&gt;</content>
</config>
