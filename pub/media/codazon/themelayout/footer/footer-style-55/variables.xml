<?xml version="1.0"?>
<config>
  <identifier>footer-style-55</identifier>
  <title>Footer Style 55</title>
  <variables>
    <footer_background>#ffffff</footer_background>
    <footer_background_file></footer_background_file>
    <footer_background_image_option></footer_background_image_option>
    <footer_background_normal>#ffffff</footer_background_normal>
    <footer_border>#ededed</footer_border>
    <footer_bottom_background>#ffffff</footer_bottom_background>
    <footer_font>14px</footer_font>
    <footer_hover_text>#5c2900</footer_hover_text>
    <footer_text>#333333</footer_text>
    <footer_text_normal>#333333</footer_text_normal>
    <footer_title_font>18px</footer_title_font>
    <footer_title_text>#000000</footer_title_text>
    <footer_top_background>#ffffff</footer_top_background>
  </variables>
  <layout_xml>&lt;body&gt;&#xD;
    &lt;referenceBlock name="footer_links" remove="true" /&gt;&#xD;
    &lt;referenceBlock name="copyright" remove="true" /&gt;&#xD;
    &lt;referenceBlock name="report.bugs" remove="true" /&gt;&#xD;
    &lt;referenceBlock name="form.subscribe" remove="true" /&gt;&#xD;
    &lt;move element="theme-layout-footer" destination="footer-container" /&gt;&#xD;
&#xD;
 &lt;block class="Magento\Store\Block\Switcher" name="footer-switch-language" template="Magento_Store::switch/languages-style-02.phtml" /&gt;&#xD;
    &lt;block class="Magento\Directory\Block\Currency" name="footer-switch-currency" template="Magento_Directory::currency-style-02.phtml" /&gt;&#xD;
&lt;/body&gt;</layout_xml>
  <custom_variables/>
  <custom_fields>
    <version>6603efa44430a</version>
    <auto_detect_files>
      <item>_instagram.less.css</item>
    </auto_detect_files>
    <custom_variables></custom_variables>
    <custom_less_code></custom_less_code>
    <custom_css_code></custom_css_code>
    <flexible_less/>
  </custom_fields>
  <parent></parent>
  <content>&lt;div class="footer-style-55"&gt;&#xD;
    &lt;div class="container-fluid"&gt;&#xD;
        &lt;div class="footer content"&gt;&#xD;
            &lt;div class="footer-top"&gt;&#xD;
