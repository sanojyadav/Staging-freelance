    &lt;/block&gt;&#xD;
        &#xD;
    &lt;referenceContainer name="header.container" htmlClass="page-header header-style-55"&gt;&#xD;
        &lt;container name="header-top" htmlTag="div" htmlClass="header-top" before="-" /&gt;&#xD;
