  <layout_xml>&lt;body&gt;&#xD;
    &lt;block name="header-extra-text-1" class="Codazon\ThemeLayoutPro\Block\Header" template="Codazon_ThemeLayoutPro::layout/header-content.phtml"&gt;&#xD;
        &lt;action method="setData"&gt;&#xD;
