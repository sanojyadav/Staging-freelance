    &lt;/referenceContainer&gt;&#xD;
    &lt;referenceContainer name="header.panel"&gt;&#xD;
        &lt;container name="header-main" htmlTag="div" htmlClass="header-main container"&gt;&#xD;
            &lt;container name="header-panel-row" htmlTag="div" htmlClass="row cdz-fix-left d-flex-center" after="skip_to_content"&gt;&#xD;
                &lt;container name="header-panel-left" htmlTag="div" htmlClass="col-sm-12 col-md-5 header-panel-left" /&gt;&#xD;
                &lt;container name="header-panel-middle" htmlTag="div" htmlClass="col-sm-24 col-md-14 header-panel-middle" /&gt;&#xD;
                &lt;container name="header-panel-right" htmlTag="div" htmlClass="col-sm-12 col-md-5 header-panel-right" /&gt;&#xD;
                &lt;container name="header-search-wrap" htmlId="header-search-wrap" htmlTag="div" htmlClass="col-sm-24 header-search-wrap" /&gt;&#xD;
            &lt;/container&gt;&#xD;
        &lt;/container&gt;&#xD;
    &lt;/referenceContainer&gt;&#xD;
    &#xD;
    &lt;referenceBlock name="top.links"&gt;&#xD;
        &lt;action method="setData"&gt;&#xD;
            &lt;argument name="name" xsi:type="string"&gt;css_class&lt;/argument&gt;&#xD;
            &lt;argument name="value" xsi:type="string"&gt;header links visible-xs&lt;/argument&gt;&#xD;
        &lt;/action&gt;&#xD;
    &lt;/referenceBlock&gt;&#xD;
    &#xD;
