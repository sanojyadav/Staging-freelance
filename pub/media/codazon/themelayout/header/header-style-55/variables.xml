            &lt;arguments&gt;&#xD;
                &lt;argument name="html_class" xsi:type="string"&gt;search-link&lt;/argument&gt;&#xD;
            &lt;/arguments&gt;&#xD;
        &lt;/block&gt;&#xD;
        &lt;block class="Magento\Framework\View\Element\Template" name="links.features.linkslist" template="Magento_Theme::html/custom-li.phtml"  after="-"&gt;&#xD;
            &lt;arguments&gt;&#xD;
                &lt;argument name="html_class" xsi:type="string"&gt;linkslist-link&lt;/argument&gt;&#xD;
            &lt;/arguments&gt;&#xD;
        &lt;/block&gt;&#xD;
    &lt;/referenceBlock&gt;&#xD;
    &lt;referenceBlock name="utilities.toggle.linkslist"&gt;&#xD;
        &lt;action method="setDirection"&gt;&#xD;
            &lt;argument name="value" xsi:type="string"&gt;right&lt;/argument&gt;&#xD;
        &lt;/action&gt;&#xD;
    &lt;/referenceBlock&gt;&#xD;
    &#xD;
    &lt;move element="header-extra-text-1" destination="header-top" before="-" /&gt;&#xD;
    &lt;move element="header-extra-text-2" destination="header-bottom" /&gt;&#xD;
